{
    "name"        : "shop-front",
    "script"      : "src/ShopFront/server.js",
    "instances"   : 1,
    "merge_logs"  : true,
    "log_date_format" : "YYYY-MM-DD HH:mm Z",
    "watch": ["src/ShopFront/server.js"],
    "watch_options": {
    "followSymlinks": true,
    "usePolling"   : true,
    "interval"    : 5
    },
    "post_update": "cd src/ShopFront && npm install"
}